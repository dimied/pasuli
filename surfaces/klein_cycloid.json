{
    "name": "Bell Polar",
    "params": [
        "a",
        "b"
    ],
    "ranges": {
        "u": [
            0,
            "2*PI"
        ],
        "v": [
            0,
            12
        ],
        "a": [
            "(0",
            "inf"
        ],
        "b": [
            "(0",
            "inf"
        ]
    },
    "examples": [
        {
            "a": 1.5,
            "b": 0.5
        }
    ],
    "vals": [
        {
            "name": "r",
            "op": "sqrt(2*v*v)"
        }
    ],
    "x": "v*cos(u)",
    "y": "v*sin(u)",
    "z": "b*exp(-(a*a*r*r))"
}