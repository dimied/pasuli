{
    "name": "Neovius Surface",
    "ranges": {
        "u": [
            "-0.5*PI",
            "0.5*PI"
        ],
        "v": [
            "-0.5*PI",
            "0.5*PI"
        ]
    },
    "vals": [
        {
            "name": "r",
            "op": "sqrt(2*v*v)"
        }
    ],
    "x": "u",
    "y": "v",
    "z": "acos((-3*(cos(u)+cos(v)))/(3+4*cos(u)*cos(v)))"
}