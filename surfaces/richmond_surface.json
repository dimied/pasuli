{
    "name": "Richmond Surface",
    "ranges": {
        "u": [
            -1,
            1
        ],
        "v": [
            -1,
            1
        ]
    },
    "vals": [
        {
            "name": "d",
            "op": "(6*(u^2 + v^2))"
        }
    ],
    "x": "(-3*u - pow(u,5) + 2*pow(u,3)*v*v + 3*u*pow(v,4))/d",
    "y": "(-3*v - 3*pow(u,4)*v - 2*pow(u,2)*pow(v,3) + pow(v,5))/d",
    "z": "u"
}