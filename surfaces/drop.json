{
    "name": "Drop",
    "params": [
        "a",
        "b"
    ],
    "ranges": {
        "u": [
            0,
            "PI"
        ],
        "v": [
            0,
            "2*PI"
        ],
        "a": [
            "(0",
            "inf"
        ],
        "b": [
            "(0",
            "inf"
        ]
    },
    "examples": [
        {
            "a": 1.5,
            "b": 0.5
        }
    ],
    "vals": [
        {
            "name": "cu",
            "op": "cos(u)"
        },
        {
            "name": "r",
            "op": "a*(b - cu)*sin(u)"
        }
    ],
    "x": "r*cos(v)",
    "y": "r*sin(v)",
    "z": "cu"
}