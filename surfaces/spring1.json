{
    "name": "Spring 1",
    "params": [
        "R",
        "r",
        "L"
    ],
    "ranges": {
        "u": [
            0,
            "2*PI"
        ],
        "v": [
            0,
            "2*PI"
        ],
        "R": [
            "(0",
            "inf"
        ],
        "r": [
            "(0",
            "inf"
        ],
        "L": [
            "(0",
            "inf"
        ]
    },
    "examples": [
        {
            "R": 1.5,
            "r": 0.5,
            "L":1
        }
    ],
    "vals": [
        {
            "name": "xy",
            "op": "(R + r*cos(v))"
        }
    ],
    "x": "xy*cos(u)",
    "y": "xy*sin(u)",
    "z": "r*(sin(v) + L*u/PI)"
}