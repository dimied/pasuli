{
    "name": "Guimard Surface",
    "params": [
        "a",
        "b",
        "c"
    ],
    "ranges": {
        "u": [
            -1,
            1
        ],
        "v": [
            -1,
            1
        ],
        "a": [
            "(0",
            "inf"
        ],
        "b": [
            "(0",
            "inf"
        ],
        "c": [
            "(0",
            "inf"
        ]
    },
    "examples": [
        {
            "a": 1.5,
            "b": 0.5,
            "c": 1
        }
    ],
    "x": "((1 - u)*a + u*b)*cos(v)",
    "y": "b*u*sin(v)",
    "z": "c*u*sin(v)*sin(v)"
}