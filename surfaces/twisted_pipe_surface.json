{
    "name": "Twisted pipe Surface",
    "ranges": {
        "u": [
            0,
            "2*PI"
        ],
        "v": [
            0,
            "2*PI"
        ]
    },
    "vals": [
        {
            "name": "C",
            "op": "2*PI/3"
        },
        {
            "name": "h",
            "op": "sqrt(1 + sin(v)*sin(v))"
        }
    ],
    "x": "sin(v - C)*(2 + cos(u - C))/h",
    "y": "sin(v + C)*(2 + cos(u + C))/h",
    "z": "cos(v)*(2 + cos(u))/h"
}