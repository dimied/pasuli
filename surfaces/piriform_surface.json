{
    "name": "Piriform Surface",
    "params": [
        "a",
        "b",
        "r"
    ],
    "ranges": {
        "u": [
            0,
            "2*PI"
        ],
        "v": [
            "-0.5*PI",
            "0.5*PI"
        ],
        "a": [
            "(0",
            "inf"
        ],
        "b": [
            "(0",
            "inf"
        ],
        "r": [
            "(0",
            "inf"
        ]
    },
    "examples": [
        {
            "a": 1.5,
            "b": 0.5,
            "r": 1
        }
    ],
    "vals": [
        {
            "name": "c",
            "op": "(r + sin(v))"
        },
        {
            "name": "d",
            "op": "b*cos(v)*c"
        }
    ],
    "x": "d*cos(u)",
    "y": "d*sin(u)",
    "z": "a*c"
}