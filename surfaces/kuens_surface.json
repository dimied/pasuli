{
    "name": "Kuens Surface",
    "ranges": {
        "u": [
            -4.3,
            4.3
        ],
        "v": [
            0.01,
            3.11
        ]
    },
    "vals": [
        {
            "name": "h",
            "op": "1 + u*u*sin(v)*sin(v)"
        }
    ],
    "x": "(2*(cos(u)+u*sin(u))*sin(v))/h",
    "y": "(2*(-u*cos(u)+sin(u))*sin(v))/h",
    "z": "log(tan(v/2)) + 2*cos(v)/h"
}