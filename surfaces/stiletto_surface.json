{
    "name": "Stiletto Surface",
    "ranges": {
        "u": [
            0,
            "2*PI"
        ],
        "v": [
            0,
            "PI"
        ]
    },
    "vals": [
        {
            "name": "c",
            "op": "2*PI/3"
        },
        {
            "name": "yz",
            "op": "cos(v+c)*cos(v+c)*sin(v+c)*sin(v+c)"
        }

        
    ],
    "x": "(2+cos(u))*cos(v)*cos(v)*cos(v)*sin(v)",
    "y": "(2+cos(u+c))*yz",
    "z": "-(2+cos(u-c))*yz"
}