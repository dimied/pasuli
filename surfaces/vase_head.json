{
    "name": "Vase Head",
    "params": [
        "a",
        "b"
    ],
    "ranges": {
        "u": [
            0,
            "2*PI"
        ],
        "v": [
            -2,
            2
        ],
        "a": [
            "(0",
            "inf"
        ],
        "b": [
            "(0",
            "inf"
        ]
    },
    "examples": [
        {
            "a": 1,
            "b": 1
        }
    ],
    "x": "v*cos(b*v + u)",
    "y": "v*sin(u)",
    "z": "a*v*v"
}