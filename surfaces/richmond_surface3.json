{
    "name": "Richmond Surface 3",
    "params": "n",
    "ranges": {
        "u": [
            0.5,
            1
        ],
        "v": [
            0,
            "2*PI"
        ],
        "n": [
            "(0",
            "inf"
        ]
    },
    "examples": [
        {
            "n": 2
        }
    ],
    "x": "-cos(v)/(2*u) - (pow(u,(2*n+1))*cos(-(2*n+1)*v))/(4*n+2)",
    "y": "-sin(v)/(2*u) + (pow(u,(2*n+1))*sin(-(2*n+1)*v))/(4*n+2)",
    "z": "pow(u,n)*cos(n*v)/n"
}