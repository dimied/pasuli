{
    "name": "Moebius Band 2",
    "params": [
        "a",
        "b",
        "m"
    ],
    "ranges": {
        "u": [
            -0.4,
            0.4
        ],
        "v": [
            0,
            "2*PI"
        ],
        "a": [
            0,
            "inf"
        ],
        "b": [
            0,
            "inf"
        ],
        "m": [
            "(0",
            "inf"
        ]
    },
    "examples": [
        {
            "a": 1,
            "b": 1,
            "m":1
        }
    ],
    "vals": [
        {
            "name": "xy",
            "op": "u*sin(0.5*m*v)"
        }
    ],
    "x": "(a + xy)*cos(v)",
    "y": "(b + xy)*sin(v)",
    "z": "u*cos(0.5*m*v)"
}