{
    "name": "Triaxial Tritorus",
    "ranges": {
        "u": [
            "-PI",
            "PI"
        ],
        "v": [
            "-PI",
            "PI"
        ]
    },
    "vals": [
        {
            "name": "C2",
            "op": "2*pi/3"
        },
        {
            "name": "C4",
            "op": "4*pi/3"
        }
    ],
    "x": "sin(u + C2)*(1 + cos(v + C2))",
    "y": "sin(u + C4)*(1 + cos(v + C4))",
    "z": "sin(u)*(1 + cos(v))"
}