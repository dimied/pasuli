{
    "name": "Sine Cone",
    "params": [
        "k",
        "N"
    ],
    "ranges": {
        "u": [
            -10,
            10
        ],
        "v": [
            "-PI",
            "PI"
        ],
        "k": [
            "(0",
            "inf"
        ],
        "N": [
            "(0",
            "inf"
        ]
    },
    "examples": [
        {
            "k": 1.5,
            "N": 0.5
        }
    ],
    "x": "u*cos(v)",
    "y": "u*sin(v)",
    "z": "k*u*cos(N*v)"
}