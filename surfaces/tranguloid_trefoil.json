{
    "name": "Tranguloid Trefoil",
    "ranges": {
        "u": [
            "-PI",
            "PI"
        ],
        "v": [
            "-PI",
            "PI"
        ]
    },
    "vals": [
        {
            "name": "C",
            "op": "2*PI/3"
        }
    ],
    "x": "2*sin(3*u)/(2 + cos(v))",
    "y": "2*(sin(u) + 2*sin(2*u))/(2 + cos(v + C))",
    "z": "(cos(u) - 2*cos(2*u))*(2 + cos(v))*(2 + cos(v + C))*0.25"
}