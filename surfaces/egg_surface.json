{
    "name": "Egg surface",
    "params": [
        "a",
        "c"
    ],
    "ranges": {
        "u": [
            0,
            "2*a"
        ],
        "v": [
            0,
            "2*PI"
        ],
        "a": [
            "(0",
            "inf"
        ],
        "c": [
            "(0",
            "inf"
        ]
    },
    "examples": [
        {
            "a": 2,
            "c": 1.5
        }
    ],
    "vals": [
        {
            "name": "r",
            "op": "0.5*sqrt(3*u*(2*a-u)*(1-c*c/((u+a)*(u+a))))"
        }
    ],
    "x": "r*cos(v)",
    "y": "r*sin(v)",
    "z": "u"
}