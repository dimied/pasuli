{
    "name": "Bour's Minimal surface",
    "vars": [
        "u",
        "v"
    ],
    "ranges": {
        "u": [
            "-PI",
            "PI"
        ],
        "v": [
            "-2*PI",
            "2*PI"
        ]
    },
    "x": "u*(cos(v)-u*cos(2*v)/2)",
    "y": "-u*(sin(v)-u*sin(2*v)/2)",
    "z": "4*u*sqrt(u)*cos(3*u/2)/3"
}