{
    "name": "Triaxial Hexatorus",
    "ranges": {
        "u": [
            0,
            "2*PI"
        ],
        "v": [
            0,
            "2*PI"
        ]
    },
    "vals": [
        {
            "name": "C",
            "op": "2*PI/3"
        },
        {
            "name": "xy",
            "op": "(sqrt(2) + cos(v + C))"
        }
    ],
    "x": "sin(u + C)/xy",
    "y": "cos(u - C)/xy",
    "z": "sin(u)/(sqrt(2) + cos(v))"
}