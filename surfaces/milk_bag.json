{
    "name": "Milk Bag",
    "params": "k",
    "ranges": {
        "u": [
            -1,
            1
        ],
        "v": [
            "-PI",
            "PI"
        ],
        "k": [
            "(0",
            "inf"
        ]
    },
    "examples": [
        {
            "k": 1
        }
    ],
    "x": "k*(1 + u)*cos(v)",
    "y": "k*(1 - u)*sin(v)",
    "z": "u"
}