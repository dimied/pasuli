{
    "name": "Hole Discus",
    "params": [
        "a",
        "b"
    ],
    "ranges": {
        "u": [
            0,
            "2*PI"
        ],
        "v": [
            0,
            "2*PI"
        ],
        "a": [
            "(0",
            "inf"
        ],
        "b": [
            "(0",
            "inf"
        ]
    },
    "examples": [
        {
            "a": 1.5,
            "b": 0.5
        }
    ],
    "vals": [
        {
            "name": "xy",
            "op": "(a + sin(v))"
        }
    ],
    "x": "sin(u)/xy",
    "y": "cos(u)/xy",
    "z": "cos(v)/b"
}