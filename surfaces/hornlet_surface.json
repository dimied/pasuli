{
    "name": "Hornlet Surface",
    "params": [
        "a",
        "b",
        "c",
        "h"
    ],
    "ranges": {
        "u": [
            0,
            2
        ],
        "v": [
            0,
            1
        ],
        "a": [
            "(0",
            "inf"
        ],
        "b": [
            "(0",
            "inf"
        ],
        "c": [
            "(0",
            "inf"
        ],
        "h": [
            "(0",
            "inf"
        ]
    },
    "examples": [
        {
            "a": 1,
            "b": 1,
            "c": 1,
            "h": 1
        }
    ],
    "vals": [
        {
            "name": "r",
            "op": "(a+sin(b*PI*u)*sin(b*PI*v))"
        }
    ],
    "x": "r*sin(c*PI*v)",
    "y": "r*cos(c*PI*v)",
    "z": "cos(b*PI*u)*sin(b*PI*v)+h*v"
}