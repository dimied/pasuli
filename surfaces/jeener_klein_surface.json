{
    "name": "Jeener Klein Surface",
    "params": [
        "S",
        "T",
        "a"
    ],
    "ranges": {
        "u": [
            0,
            "2*PI"
        ],
        "v": [
            0,
            "2*PI"
        ],
        "S": [
            "(0",
            "inf"
        ],
        "T": [
            "(0",
            "inf"
        ],
        "a": [
            "(0",
            "inf"
        ]
    },
    "examples": [
        {
            "S": 1.5,
            "T": 0.5,
            "a":1
        }
    ],
    "vals": [
        {
            "name": "W",
            "op": "((S + 1)/4)*cos((S + 1)*u +pi/T) + sqrt(a)"
        }
    ],
    "x": "S*cos(u) + cos(S*u) - W*sin((S - 1)*u/2)*cos(v)",
    "y": "S*sin(u) - sin(S*u) - W*cos((S - 1)*u/2)*cos(v)",
    "z": "W*sin(v)"
}