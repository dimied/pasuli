{
    "name": "Pisot Triaxial",
    "ranges": {
        "u": [
            0,
            "2*PI"
        ],
        "v": [
            0,
            "2*PI"
        ]
    },
    "x": "0.655866*cos(1.03002 + u)*(2 + cos(v))",
    "y": "0.74878*cos(1.40772 - u)*(2 + 0.868837*cos(2.43773 + v))",
    "z": "0.868837*cos(2.43773 + u)*(2 + 0.495098*cos(0.377696 - v))"
}