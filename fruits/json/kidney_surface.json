{
    "name": "Kidney Surface",
    "vars": [
        "u",
        "v"
    ],
    "ranges": {
        "u": [
            0,
            "2*PI"
        ],
        "v": [
            "-0.5*PI",
            "0.5*PI"
        ]
    },
    "vals": [
        {
            "name": "xy",
            "op": "(3*cos(v) - cos(3*v))"
        }
    ],
    "x": "xy*cos(u)",
    "y": "xy*sin(u)",
    "z": "3*sin(v) - sin(3*v)"
}