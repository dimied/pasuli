{
    "name": "Ellipsoid",
    "params": [
        "a",
        "b",
        "c"
    ],
    "ranges": {
        "u": [
            0,
            "PI"
        ],
        "v": [
            0,
            "2*PI"
        ],
        "a": [
            "(0",
            "inf"
        ],
        "b": [
            "(0",
            "inf"
        ],
        "c": [
            "(0",
            "inf"
        ]
    },
    "examples": [
        {
            "a": 1.5,
            "b": 1,
            "c": 1
        }
    ],
    "x": "a*sin(u)*cos(v)",
    "y": "b*sin(u)*sin(v)",
    "z": "c*cos(u)"
}