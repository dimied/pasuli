{
    "name": "Sphere uv",
    "vars": [
        "u",
        "v"
    ],
    "ranges": {
        "u": [
            0,
            "inf"
        ],
        "v": [
            "-inf",
            "inf"
        ]
    },
    "vals": {
        "uq": "u*u",
        "vq": "v*v",
        "up": "uq+1",
        "vp": "vq+1"
    },
    "x": "(2*u*(1-vq))/(up*vp)",
    "y": "(1-uq)/up",
    "z": "(4*u*v)/(up*vp)"
}