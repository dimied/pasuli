{
    "name": "Wrinkle sphere 2",
    "params": [
        "n"
    ],
    "vars": [
        "u",
        "v"
    ],
    "ranges": {
        "u": [
            0,
            "PI"
        ],
        "v": [
            "(0",
            "2*PI)"
        ],
        "n": [
            1,
            "inf"
        ]
    },
    "x": "sin(v)*sin(u)+(cos(n*v)/n)",
    "y": "sin(v)*cos(u)+(cos(n*u)/n)",
    "z": "cos(v)"
}