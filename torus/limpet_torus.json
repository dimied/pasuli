{
    "name": "Limpet Torus",
    "ranges": {
        "u": [
            0,
            "2*PI"
        ],
        "v": [
            0,
            "2*PI"
        ]
    },
    "vals": [
        {
            "name": "xy",
            "op": "(sqrt(2) + sin(v))"
        }
    ],
    "x": "cos(u)/xy",
    "y": "sin(u)/xy",
    "z": "1/(sqrt(2) + cos(v))"
}