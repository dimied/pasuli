{
    "name": "Piriform Torus 1",
    "params": [
        "R",
        "r"
    ],
    "ranges": {
        "u": [
            0,
            "2*PI"
        ],
        "v": [
            0,
            "2*PI"
        ],
        "R": [
            "(0",
            "inf"
        ],
        "r": [
            "(0",
            "inf"
        ]
    },
    "examples": [
        {
            "R": 1,
            "r": 1
        }
    ],
    "vals": [
        {
            "name": "F",
            "op": "(R+(r+sin(v)))"
        }
    ],
    "x": "F*cos(u)",
    "y": "F*sin(u)",
    "z": "cos(v)*(r+sin(v))"
}