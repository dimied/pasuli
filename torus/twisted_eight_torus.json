{
    "name": "Twisted Eight Torus",
    "params": [
        "R",
        "r"
    ],
    "ranges": {
        "u": [
            0,
            "2*PI"
        ],
        "v": [
            0,
            "2*PI"
        ],
        "R": [
            "(0",
            "inf"
        ],
        "r": [
            "(0",
            "inf"
        ]
    },
    "examples": [
        {
            "R": 1.5,
            "r": 0.5
        }
    ],
    "vals": [
        {
            "name": "F",
            "op": "(R + r*(cos(u*0.5)*sin(v) - sin(u*0.5)*sin(2*v)))"
        }
    ],
    "x": "F*cos(u)",
    "y": "F*sin(u)",
    "z": "r*(sin(u*0.5)*sin(v) + cos(u*0.5)*sin(2*v))"
}