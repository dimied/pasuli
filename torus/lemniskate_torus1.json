{
    "name": "Lemniskate Torus 1",
    "params": [
        "R",
        "r"
    ],
    "ranges": {
        "u": [
            0,
            "2*PI"
        ],
        "v": [
            0,
            "2*PI"
        ],
        "R": [
            "(0",
            "inf"
        ],
        "r": [
            "(0",
            "inf"
        ]
    },
    "examples": [
        {
            "R": 1.5,
            "r": 0.5
        }
    ],
    "vals": [
        {
            "name": "F",
            "op": "r*cos(v)/(1 + sin(v)*sin(v))"
        }
    ],
    "x": "(R + F)*cos(u)",
    "y": "(R + F)*sin(u)",
    "z": "sin(v)*F"
}