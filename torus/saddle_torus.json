{
    "name": "Saddle Torus",
    "ranges": {
        "u": [
            0,
            "2*PI"
        ],
        "v": [
            0,
            "2*PI"
        ]
    },
    "vals": [
        {
            "name": "C",
            "op": "2*PI/3"
        },
        {
            "name": "Fu",
            "op": "1-cos(u)*cos(u) - cos(u + C)*cos(u + C)"
        },
        {
            "name": "Fv",
            "op": "1-cos(v)*cos(v) - cos(v + C)*cos(v + C)"
        }
    ],
    "x": "(2 + cos(u))*cos(v)",
    "y": "(2 + cos(u + C))*cos(v + C)",
    "z": "(2 + sign(Fu)*sqrt(abs(Fu)))*sign(Fv)*sqrt(abs(Fv))"
}