{
    "name": "Strophoide-Cylinder",
    "params": [
        "R",
        "a"
    ],
    "vars": [
        "u",
        "v"
    ],
    "ranges": {
        "u": [
            "-PI",
            "PI"
        ],
        "v": [
            "-inf",
            "inf"
        ],
        "r": [
            "(0",
            "inf"
        ]
    },
    "vals": [
        {
            "name": "vq",
            "op": "v*v"
        },
        {
            "name": "vp1",
            "op": "vq+1"
        },
        {
            "name": "vm1",
            "op": "vq-1"
        },
        {
            "name": "xy",
            "op": "(R + a*(vm1)/(vp1))"
        }
    ],
    "x": "xy*cos(u)",
    "y": "xy*sin(u)",
    "z": "a*v*(vm1)/(vp1)"
}